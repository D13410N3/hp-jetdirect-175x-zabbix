        prtInputMediaDimXFeedDirDeclared
            INTEGER (-2147483648..2147483647),

        prtInputMediaDimFeedDirChosen
            INTEGER (-2147483648..2147483647),

        prtInputMediaDimXFeedDirChosen
            INTEGER (-2147483648..2147483647),

        prtInputCapacityUnit
            CapacityUnit,

        prtInputMaxCapacity
            INTEGER (-2147483648..2147483647),

        prtInputCurrentLevel
